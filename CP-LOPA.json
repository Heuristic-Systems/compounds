      ],
      "element_instructions": {
        "E-L01-PER-BRIDGE": {
          "instruction": "compute_trait_baselines",
          "purpose": "Transform personality traits into state-modulating parameters"
        },
        "E-L01-CONTEXT-BRIDGE": {
          "instruction": "transform_ecological_context",
          "purpose": "Transform CLEA context_vector into L01-consumable constraints"
        },
        "E-L01-MAI-BUNDLE": {
          "instruction": "measure_motivational_state",
          "purpose": "Gather motivational-affective evidence"
        },
        "E-L01-MC-BUNDLE": {
          "instruction": "measure_metacognitive_state",
          "purpose": "Gather metacognitive and self-regulation evidence"
        },
        "E-L01-PRC-BUNDLE": {
          "instruction": "measure_processing_capacity",
          "purpose": "Gather cognitive load and capacity evidence"
        },
        "E-L01-EF-BUNDLE": {
          "instruction": "measure_error_processing",
          "purpose": "Gather error and feedback processing evidence"
