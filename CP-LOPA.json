          "instruction": "measure_contextual_constraints",
          "purpose": "Check task structure and constraints"
        },
        "E-L01-MAI-BUNDLE": {
          "instruction": "assess_affective_risk",
          "purpose": "Check motivational fit and frustration risk"
        }
      },
      "execution_flow": [
        "1. Read K2 framework for gating rules",
        "2. Receive learner_state_profile from CP1",
        "3. Instruct E-L01-PER-BRIDGE: get_personality_context",
        "4. Receive personality-adjusted thresholds and preferences",
        "5. Instruct E-L01-MC-BUNDLE: check_goal_alignment",
        "6. Instruct E-L01-REP-BUNDLE: assess_schema_integrity",
        "7. Instruct E-L01-TLD-BUNDLE: evaluate_transfer_readiness",
        "8. Instruct E-L01-CS-BUNDLE: measure_contextual_constraints",
        "9. Instruct E-L01-MAI-BUNDLE: assess_affective_risk",
        "10. Receive evidence bundles from all Elements",
        "11. Parse objective requirements (specificity, prerequisites)",
        "12. Compute zpd_alignment using K2 formula with personality-adjusted margins",
        "13. Check prerequisite coverage against thresholds",
        "14. Assess motivational fit with personality-informed frustration tolerance",
        "15. Apply interpretation_logic with personality adjustments",
        "16. If CORRECTION_REQUIRED, generate personality-aware adjustment specification"
      ],
      "interpretation_logic": {
        "PASS": {
          "condition": "zpd_aligned AND prerequisites_met AND motivational_fit_personality_adjusted",
          "action": "approve_objective"
        },
        "CORRECTION_REQUIRED": {
          "condition": "zpd_aligned AND (prerequisites_partial OR motivational_marginal)",
          "action": "suggest_objective_adjustments_personality_informed"
        },
        "FAIL": {
          "condition": "NOT zpd_aligned OR prerequisites_severely_unmet OR (high_N_score AND objective_too_challenging)",
          "action": "reject_objective_with_personality_aware_alternative"
        },
        "personality_adjustments": {
          "high_neuroticism": "widen safety margin for challenge level",
          "high_openness": "allow stretch objectives beyond typical ZPD",
          "low_conscientiousness": "require clearer milestones and structure",
          "high_extraversion": "favour collaborative objectives"
        }
      },
      "outputs": {
        "primary": "objective_decision",
        "primary_values": [
          "PASS",
          "CORRECTION_REQUIRED",
          "FAIL"
        ],
        "secondary": [
          "fit_score",
          "zpd_alignment_score",
          "prerequisite_coverage",
          "adjustment_specification"
        ]
      },
      "loop_policy": {
        "reinterpretation_max": 5,
        "more_data_max": 2,
        "escalation_condition": "Cannot satisfy Objective Gating requirements within bounded loops."
