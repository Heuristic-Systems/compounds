        "E-L01-TLD-BUNDLE": {
          "instruction": "measure_temporal_dynamics",
          "purpose": "Gather temporal learning dynamics evidence"
        },
        "E-L01-CS-BUNDLE": {
          "instruction": "measure_contextual_constraints",
          "purpose": "Gather contextual structure evidence"
        },
        "E-L01-GEN-BUNDLE": {
          "instruction": "measure_system_state",
          "purpose": "Gather general system evidence"
        },
        "E-L01-MML-BUNDLE": {
          "instruction": "measure_modality_preferences",
          "purpose": "Gather multimodal preferences for L05 handoff"
        }
      },
      "execution_flow": [
        "1. Read K1 framework for synthesis rules",
        "2. If personality_profile_signature available, instruct E-L01-PER-BRIDGE: compute_trait_baselines",
        "3. Receive personality_state_modifiers (baselines, thresholds, preferences)",
        "4. Instruct all 9 state Elements with their respective instructions",
        "5. Receive evidence bundles from all state Elements",
        "6. Validate evidence quality (must pass CP7 first)",
        "7. Apply personality_state_modifiers to weight/interpret state evidence",
        "8. Normalise and timestamp-weight all signals",
        "9. Compute per-family state estimates with personality-adjusted confidence",
        "10. Run cross-family contradiction detection (personality-aware)",
        "11. Extract modality_preference_profile from MML for L05 (personality-informed)",
        "12. Aggregate into unified learner_state_vector with trait context",
        "13. Apply interpretation_logic",
        "14. Emit learner_state_vector with personality_state_modifiers"
      ],
      "interpretation_logic": {
        "VALID_PROFILE": {
          "condition": "core_family_coverage >= 3 AND aggregate_confidence >= 0.5",
          "action": "emit_learner_state_vector"
        },
        "INCOMPLETE_PROFILE": {
          "condition": "core_family_coverage < 3 OR aggregate_confidence < 0.5",
          "action": "request_additional_evidence"
        },
        "contradiction_handling": {
          "condition": "family_score_delta > 30",
          "action": "flag_contradiction_and_weight_by_recency"
        }
      },
      "outputs": {
        "primary": "learner_state_vector",
        "secondary": [
          "confidence_scores",
          "contradiction_flags",
          "modality_preference_profile",
          "personality_state_modifiers"
        ],
        "handoff": {
          "modality_preference_profile": {
            "target_layer": "L05",
            "target_agent": "MSA"
          }
        }
      },
      "loop_policy": {
        "reinterpretation_max": 5,
        "more_data_max": 2,
        "escalation_condition": "Cannot satisfy State Synthesis requirements within bounded loops."
      },
      "upstream": [],
      "downstream": [
        "PH-LOPA-3"
      ]
    },
    {
